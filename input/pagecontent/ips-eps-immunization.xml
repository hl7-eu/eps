        <tr>
          <td class="cell-iso lvl-1">Product administered</td>
          <td class="cell-iso">O</td>
          <td class="cell-mid">Equivalent</td>
          <td class="cell-eu">administeredProduct</td>
          <td class="cell-eu note">Administered medicinal product</td>
        </tr>
        <tr>
          <td class="cell-iso lvl-2">Brand name</td>
          <td class="cell-iso">RK</td>
          <td class="cell-mid">source-is-narrower-than-target</td>
          <td class="cell-eu">administeredProduct</td>
          <td class="cell-eu note">Administered medicinal product</td>
