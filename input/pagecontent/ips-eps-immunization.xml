        <td class="cell-iso lvl-1">Series doses</td>
        <td class="cell-iso">O</td>
        <td class="cell-mid">Equivalent</td>
        <td class="cell-eu">protocolApplied.seriesDoses[x]</td>
        <td class="cell-eu note">Planned number of doses in series.</td>
      </tr>
      <tr>
        <td class="cell-iso lvl-1">Target disease</td>
        <td class="cell-iso">RK</td>
        <td class="cell-mid">Equivalent</td>
        <td class="cell-eu">protocolApplied.targetDisease</td>
        <td class="cell-eu note">Disease the vaccination provides protection against.</td>
